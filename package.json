{
  "name": "oro-sentry",
  "version": "2.0.0",
  "description": "OSentry Class is a wrapper of @sentry/node that allow to use Sentry and send custom events.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -R ./dist",
    "clean:all": "rm -R ./dist ./node_modules",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean"
  },
  "keywords": [
    "sentry",
    "promise",
    "async/await",
    "typescript"
  ],
  "author": "Carlos Oropesa <carlos@oropensando.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oropesa/oro-sentry.git"
  },
  "bugs": {
    "url": "https://github.com/oropesa/oro-sentry/issues"
  },
  "homepage": "https://github.com/oropesa/oro-sentry#readme",
  "dependencies": {
    "@sentry/integrations": "^7.77.0",
    "@sentry/node": "^7.77.0",
    "oro-functions": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "sentry-testkit": "^5.0.6",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "wait-for-expect": "^3.0.2"
  }
}
